Given a list of edges Do DFS on undirected graph to detect cycle

#include <bits/stdc++.h>
using namespace std;
int cycle;
void isCycle(vector<vector<int>>& edge, vector<int>&vis,int cur)
{
	vis[cur]=1;
	for(auto x:edge[cur])
	{
		if(!vis[x])
			dfs(edge,vis,x);
		else if(vis[x]==2)
			cycle=1;
	}
	vis[cur]=1;
}
void dfs()
{
	int n,m;
	cin>>n>>m;
	cycle=0;
	vector<vector<int>>edge(n);
	for(int i=0;i<m;i++)
	{
		int u,v;
		edge[u].push_back(v);
		edge[v].push_back(u);
	}
	vector<int>vis(n,0);
	isCycle(edge,vis,0);
	cout<<cycle;
}
int main()
{
	int t;
	while(t--)
		dfs();
}